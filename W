local OriginalAnimations = {
    ["Idle"] = {
        ["2016 Animation (mm2)"] = {"387947158", "387947464"},
        ["Bubbly"] = {"910004836", "910009958"},
    },
    ["Walk"] = {
        ["Bubbly"] = "910034870",
        ["Cartoony"] = "742640026",
    },
    ["Run"] = {
        ["Bubbly"] = "10921057244",
        ["Cartoony"] = "10921076136",
    },
    ["Jump"] = {
        ["Bubbly"] = "910016857",
        ["Cartoony"] = "742637942",
    },
    ["Fall"] = {
        ["Bubbly"] = "910001910",
        ["Cartoony"] = "742637151",
    },
    ["Climb"] = {
        ["Bubbly"] = "910001910",
    }
